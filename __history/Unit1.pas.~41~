    zzb[20,an[2]]:=false;
    zzb[21,an[2]]:=false;
    zzb[10,an[3]]:=false;
    zzb[11,an[3]]:=false;
    zzb[12,an[3]]:=false;
    zzb[19,an[3]]:=false;
    zzb[20,an[3]]:=false;
    zzb[21,an[3]]:=false;
