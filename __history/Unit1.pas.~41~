    for d := 1 to zzd[0] do  zzb[zzd[d],j]:=true;

    end;
  newarray:=false; 
  exit;                                                                                                 
 end;
 function isright():boolean;
 function check(c1,c2,c3,c4,c5,c6,c7,c8,c9:integer):boolean;
  var c,h1,h2,h3,h4,h5,h6,h7,h8,h9:set of byte;
  begin
   c:=[1,2,3,4,5,6,7,8,9];
   h1:=[1];
   h2:=[2];
   h3:=[3];
   h4:=[4];
   h5:=[5];
   h6:=[6];
   h7:=[7];
   h8:=[8];
   h9:=[9];
 case c1 of
   0:begin end;
   1:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h1;end;
   2:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h2;end;
   3:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h3;end;
   4:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h4;end;
   5:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h5;end;
   6:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h6;end;
   7:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h7;end;
   8:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h8;end;
   9:begin if not(c1 in c) then begin check:=true;exit;end;c:=c-h9;end;
   else begin check:=true;exit; end;
   end;
     case c2 of
     0:begin end;
   1:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h1;end;
   2:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h2;end;
   3:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h3;end;
   4:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h4;end;
   5:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h5;end;
   6:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h6;end;
   7:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h7;end;
   8:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h8;end;
   9:begin if not(c2 in c) then begin check:=true;exit;end;c:=c-h9;end;
   else begin check:=true;exit; end;
   end;
      case c3 of
      0:begin end;
   1:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h1;end;
   2:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h2;end;
   3:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h3;end;
   4:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h4;end;
   5:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h5;end;
   6:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h6;end;
   7:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h7;end;
   8:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h8;end;
   9:begin if not(c3 in c) then begin check:=true;exit;end;c:=c-h9;end;
   else begin check:=true;exit; end;
   end;
     case c4 of
