function newarray(i:integer):boolean;
 var zzd:array[0..27]of integer; j,d:integer;
 begin
  if(i=82) then begin newarray:=true;exit;end;
  for j := 1 to 9 do
    if(zzb[i,j]) 
    then begin 
    zzd[0]:=0;
    an[i]:=j;
    d:=i;
    //同列
    while d+9<=81 do begin if(zzb[d+9,j]) then begin zzb[d+9,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=d+9;end;d:=d+9;end;
    //同行
    d:=i;
    while ((d+1) mod 9 <>1) do  begin if(zzb[d+1,j]) then begin zzb[d+1,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=d+1;end;d:=d+1;end;
    //同大格
    if ((i=1) or (i=2) or (i=3) or (i=10) or (i=11) or (i=12) or (i=19) or (i=20) or (i=21)) then
