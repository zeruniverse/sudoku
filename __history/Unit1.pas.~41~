 procedure getnewarray() ;
 var i,k,j,d:integer;
 begin   
   randomize;
   for i:=1 to 81 do an[i]:=0;
   for i := 1 to 9 do  zza[i]:=i;
   for i := 9 downto 1 do
   begin
   k:=random(i)+1;
   an[i]:=zza[k];
   for j := k to i-1 do  zza[j]:=zza[j+1];
   end;
   for i := 10 to 81 do
     for j := 1 to 9 do  zzb[i,j]:=true;
   for j := 1 to 9 do
   begin
    //同列
    zzb[j+9,an[j]]:=false;
    zzb[j+18,an[j]]:=false;
    zzb[j+27,an[j]]:=false;
    zzb[j+36,an[j]]:=false;
    zzb[j+45,an[j]]:=false;
    zzb[j+54,an[j]]:=false;
    zzb[j+63,an[j]]:=false;
    zzb[j+72,an[j]]:=false;
    //同格/
   end;
    //同格/
    zzb[10,an[1]]:=false;
    zzb[11,an[1]]:=false;
    zzb[12,an[1]]:=false;
    zzb[19,an[1]]:=false;
    zzb[20,an[1]]:=false;
    zzb[21,an[1]]:=false;
    zzb[10,an[2]]:=false;
    zzb[11,an[2]]:=false;
