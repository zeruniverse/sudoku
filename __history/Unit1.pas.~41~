    begin
      if(zzb[34,j]) then begin  zzb[34,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=34;end;
      if(zzb[35,j]) then begin  zzb[35,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=35;end;
      if(zzb[36,j]) then begin  zzb[36,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=36;end;
      if(zzb[43,j]) then begin  zzb[43,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=43;end;
      if(zzb[44,j]) then begin  zzb[44,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=44;end;
      if(zzb[45,j]) then begin  zzb[45,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=45;end;
      if(zzb[52,j]) then begin  zzb[52,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=52;end;
      if(zzb[53,j]) then begin  zzb[53,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=53;end;
      if(zzb[54,j]) then begin  zzb[54,j]:=false; zzd[0]:=zzd[0]+1; zzd[zzd[0]]:=54;end;
