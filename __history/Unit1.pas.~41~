procedure TForm1.Timer1Timer(Sender: TObject);
begin
if time=0 then begin time:=1; edit83.text:='△欢迎传播▲';
end else begin time:=0;edit83.Text:='版权没有^_^';
end;

end;

procedure TForm1.Timer2Timer(Sender: TObject);
var s,t:integer;
begin
inc(ttime);
 s:=strtoint(edit86.Text);
 if edit86.Text='59' then begin
  edit86.Text:='0';
  if edit85.Text='59' then begin  t:=strtoint(edit84.Text);
  inc(t);edit84.Text:=inttostr(t); edit85.Text:='0';exit;
  end;
  t:=strtoint(edit85.Text);
  inc(t);edit85.Text:=inttostr(t);exit;
 end;
 inc(s);
 edit86.Text:=inttostr(s);
end;

procedure TForm1.Button3Click(Sender: TObject);
var i:integer;a:array[1..81]of integer;
begin
timer2.Enabled:=false;
ttime:=0;
button2.Enabled:=false;
button3.Enabled:=false;
 for i := 1 to 81 do   begin a[i]:=an[i];
   case i of
     1:begin edit1.text:=inttostr(a[1]);  edit1.Enabled:=false;
    end;
    2:begin edit2.text:=inttostr(a[2]);  edit2.Enabled:=false;
    end;
